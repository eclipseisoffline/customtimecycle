{
  "schemaVersion": 1,
  "id": "customtimecycle",
  "version": "${version}",
  "name": "Custom Time Cycle",
  "description": "Allows the day and night durations of Minecraft days to be customised, without changing tick speed.",
  "authors": [
    {
      "name": "eclipseisoffline",
      "contact": {
        "homepage": "https://github.com/eclipseisoffline"
      }
    }
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/customtimecycle",
    "issues": "https://github.com/eclipseisoffline/customtimecycle/issues",
    "sources": "https://github.com/eclipseisoffline/customtimecycle"
  },
  "license": "LGPL-3.0-only",
  "icon": "assets/customtimecycle/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [],
    "main": [
      "xyz.eclipseisoffline.customtimecycle.CustomTimeCycle"
    ]
  },
  "mixins": [
    "customtimecycle.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${supported_versions}",
    "fabric-permissions-api-v0": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.donate": "https://buymeacoffee.com/eclipseisoffline",
        "modmenu.buymeacoffee": "https://buymeacoffee.com/eclipseisoffline",
        "modmenu.modrinth": "https://modrinth.com/mod/customtimecycle"
      }
    }
  }
}
